{
  "name": "nori-cli",
  "version": "1.2.0",
  "description": "整理一些工作中常用的命令",
  "repository": "https://github.com/Simlesos/nori-cli",
  "keywords": [
    "nori",
    "cli"
  ],
  "main": "src/index.js",
  "bin": {
    "nori": "src/index.js"
  },
  "scripts": {
    "test": "standard",
    "prepublish": "npm run test",
    "postinstall": "node ./npm/install"
  },
  "standard": {
    "ignore": [
      "dist/*"
    ]
  },
  "ava": {
    "babel": {
      "presets": [
        "@ava/stage-4"
      ],
      "plugins": [
        [
          "transform-react-jsx",
          {
            "pragma": "h",
            "useBuiltIns": true
          }
        ]
      ]
    }
  },
  "author": "zhiqiang@guanghe.tv",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "chalk": "^2.3.0",
    "color": "^3.0.0",
    "color-string": "^1.5.2",
    "commander": "^2.14.1",
    "copy-paste": "^1.3.0",
    "inquirer": "^5.1.0",
    "ip": "^1.1.5",
    "js-yaml": "^3.11.0",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.11",
    "opn": "^5.3.0",
    "ramda": "^0.25.0",
    "rxjs": "^5.5.6",
    "shelljs": "^0.8.1",
    "ssh2": "^0.6.0"
  }
}
